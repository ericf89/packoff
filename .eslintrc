{
  "env": {
    "node": true,
    "browser": true,
  },
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "rules": {
    "no-restricted-syntax": 0,
    "no-await-in-loop": 0,
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_", "varsIgnorePattern": "^_", "ignoreRestSiblings": true }],
    "no-param-reassign": ["error", { "props": false }]
  }
}